{
  "name": "Butelka Wineshop",
  "version": "1.0.0",
  "description": "Butelka Wineshop intricate ecommerce application",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:analyze": "cross-env NODE_OPTIONS=--no-deprecation ANALYZE=true next build",
    "build:profile": "cross-env NODE_OPTIONS=--no-deprecation NEXT_PROFILING=true next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "seed:countries": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/seed-wine-countries.ts",
    "seed:tags": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/seed-tags.ts",
    "seed:grape-varieties": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/seed-grape-varieties.ts",
    "seed:regions": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/seed-wine-regions.ts",
    "seed:wineries": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/seed-wineries.ts",
    "seed:moods": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/seed-moods.ts",
    "seed:all": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/seed-all.ts"
  },
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@conform-to/react": "^1.7.0",
    "@conform-to/zod": "^1.7.0",
    "@hookform/resolvers": "^5.1.1",
    "@payloadcms/db-postgres": "3.42.0",
    "@payloadcms/next": "3.42.0",
    "@payloadcms/payload-cloud": "3.42.0",
    "@payloadcms/richtext-lexical": "3.42.0",
    "@payloadcms/storage-s3": "~3.42.0",
    "@payloadcms/ui": "3.42.0",
    "@react-email/components": "^0.1.0",
    "@react-email/tailwind": "^1.0.5",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express-rate-limit": "^7.5.0",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "next": "15.3.0",
    "next-auth": "^4.24.11",
    "next-intl": "^4.1.0",
    "payload": "3.42.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.58.1",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "resend": "^4.6.0",
    "sharp": "0.32.6",
    "stripe": "^18.2.1",
    "swiper": "^11.2.8",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "typesense": "^2.0.3",
    "yup": "^1.6.1",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250617.0",
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.3.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.32",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "5.7.3",
    "wrangler": "^4.20.1"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
